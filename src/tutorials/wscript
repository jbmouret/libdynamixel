def build(bld):
    obj = bld.program(features='cxx',
                      source='high_level.cpp',
                      includes='. ..',
                      target='high_level_protocol2')

    obj = bld.program(features='cxx',
                      source='high_level.cpp',
                      includes='. ..',
                      target='high_level_protocol1',
                      defines=['PROTOCOL1'])

    obj = bld.program(features='cxx',
                      source='low_level.cpp',
                      includes='. ..',
                      target='low_level')
